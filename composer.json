{
    "name": "nullref/yii2-core",
    "type": "yii2-extension",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.0||>=7.0",
        "yiisoft/yii2": ">=2.0.6",
        "symfony/filesystem": "^5.1",
        "symfony/finder": "^3.1",
        "voskobovich/yii2-linker-behavior": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "nullref\\core\\": "src/"
        }
    },
    "extra": {
        "bootstrap":"nullref\\core\\Bootstrap"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require-dev": {
        "yiisoft/yii2-gii": "^2.0@dev"
    }
}
